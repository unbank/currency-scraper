{
    "name": "unbank/currency-scraper",
    "description": "Fetch crypto currency exchange rate",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Oshane Bailey",
            "email": "b4.oshany@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0",
        "osoobe/laravel-utilities": "*"
    },
    "autoload": {
        "psr-4": {
            "Unbank\\CurrencyScraper\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unbank\\CurrencyScraper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Unbank\\CurrencyScraper\\CurrencyScraperServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
